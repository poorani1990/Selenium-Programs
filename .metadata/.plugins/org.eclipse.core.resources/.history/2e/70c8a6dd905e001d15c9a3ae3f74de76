package org.TestNG;

import org.junit.Before;
import org.testng.annotations.AfterMethod;
import org.testng.annotations.AfterSuite;
import org.testng.annotations.AfterTest;
import org.testng.annotations.BeforeClass;
import org.testng.annotations.BeforeMethod;
import org.testng.annotations.BeforeSuite;
import org.testng.annotations.BeforeTest;
import org.testng.annotations.Test;

public class NewTest1 {
 
@BeforeSuite
private void tc_1() {
System.out.println("Class A tc_1@BeforeSuite");
}

@AfterSuite 	
private void tc_2() {
System.out.println("Class A tc_1@AfterSuite");
}

@BeforeTest
private void tc_3() {
System.out.println("Class A tc_1@BeforeTest");
}

@AfterTest
private void tc_4() {
System.out.println("Class A tc_1@AfterTest");
}

@BeforeMethod
private void tc_5() {
System.out.println("Class A tc_1 @BeforeMethod");
}

@AfterMethod
private void tc_6() {
System.out.println("Class A tc_1@AfterMethod");
}

@BeforeClass
private void tc_7() {
System.out.println("Class A tc_1@BeforeClass");
}


}

