import org.test.BaseClass;

public class LoginPom extends BaseClass {
	
	
	public LoginPom() {
	 PageFactory.initElements(driver, this);	
		
	}
	
	@FindBy()
	private WebElement userName;
	
	@FindBy()
	private WebElement password;

}
