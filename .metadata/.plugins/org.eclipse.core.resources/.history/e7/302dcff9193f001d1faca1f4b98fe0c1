package org.test;

public class ReturnClassC {

	public static void main(String[] args) {
		ReturnTypeClassA x = new ReturnTypeClassA();
		int add = x.add(10, 20);
		System.out.println(add);
		
		if(add==30)
		{
			System.out.println("Valid");
		}
		else
		{
			System.out.println("Invalid");
		}
	
	}
}
